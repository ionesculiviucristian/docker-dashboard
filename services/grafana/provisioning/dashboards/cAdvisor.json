   "schemaVersion": 39,
   "templating": {
      "list": [
         {
            "datasource": {
               "type": "datasource",
               "uid": "prometheus"
            },
            "includeAll": true,
            "label": "Host",
            "name": "host",
            "query": "label_values({__name__=~\"container.*\"}, instance)",
            "refresh": 1,
            "type": "query"
         },
         {
            "datasource": {
               "type": "datasource",
               "uid": "prometheus"
            },
            "includeAll": true,
            "label": "Container",
            "name": "container",
            "query": "label_values({__name__=~\"container.*\", instance=~\"$host\"},name)",
            "refresh": 1,
            "type": "query"
         }
      ]
   },
   "time": {
