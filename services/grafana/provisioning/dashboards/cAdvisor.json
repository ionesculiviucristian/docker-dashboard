         "id": 1,
         "title": "CPU",
         "type": "row"
      },
      {
         "datasource": {
            "type": "datasource",
            "uid": "-- Mixed --"
         },
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "drawStyle": "line",
                  "fillOpacity": 10,
                  "lineInterpolation": "linear",
                  "scaleDistribution": {
                     "type": "linear"
                  },
                  "stacking": {
                     "group": "A",
                     "mode": "normal"
                  }
               },
               "thresholds": {
                  "mode": "absolute",
                  "steps": [
                     {
                        "color": "green"
                     },
                     {
                        "color": "red",
                        "value": 80
                     }
                  ]
               },
               "unit": "percent"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 24,
            "x": 0,
            "y": 1
         },
         "id": 2,
         "options": {
            "legend": {
               "calcs": [
                  "mean",
                  "max"
               ],
               "displayMode": "table",
               "placement": "right",
               "showLegend": true
            },
            "tooltip": {
               "mode": "multi"
            }
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "prometheus"
               },
               "expr": "sum(rate(container_cpu_usage_seconds_total{instance=~\"$host\",name=~\"$container\",name=~\".+\"}[5m])) by (name) *100",
               "legendFormat": "{{name}}"
            }
         ],
         "title": "CPU Usage",
         "type": "timeseries"
      },
